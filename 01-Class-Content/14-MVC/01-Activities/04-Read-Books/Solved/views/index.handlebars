<h1>Library Books</h1>

<h2>Books you've already read</h2>
<!-- Replace the HTML code of the book details by importing the Handlebars partial -->
<ul>
  {{#each library}}
  {{#if read}}
  {{> books/book-block}}
  {{/if}}
  {{/each}}
</ul>

<h2>Books you haven't read yet</h2>
<!-- Replace the HTML code of the book details by importing the Handlebars partial -->
<ul>
  {{#each library}}
  {{#unless read}}
  {{> books/book-block}}
  {{/unless}}
  {{/each}}
</ul>